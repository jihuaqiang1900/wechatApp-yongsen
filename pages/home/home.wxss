/* pages/home/home.wxss */
.slide-image {
  width: 100%;
  height: 150px;
}
.headerLocation {
    position: relative;
    height: 45px;
    line-height: 45px;
    font-size:12px;
    color:#666;
    padding-left:32px;
    background-color: #f6f6f6;
    margin-bottom:1px;
}
.locationImage{
    position:absolute;
    left: 10px;
    top:12px;
    width: 18px;
    height: 18px;
}
.title{
    padding:0 20px;
    background-color: #f1f1f1;
    height: 40px;
    line-height: 40px;
    font-size: 16px;
    color: #666;
    font-family:"pingfang"
}
.content{
    padding:0 20px;
    background-color: #fff;
    min-height: 30px;
    line-height: 30px;
    font-size: 14px;
    color: #666;
    font-family:"pingfang"
}
.prize_contain{
    padding: 0 20px;
}
.prize_photo{
    width: calc(50% - 20px);
    height: 100px;
    margin: 10px 5px;
}